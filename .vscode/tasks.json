{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run function tests",
			"type": "shell",
			"command": "./test.sh",
			"args": [
				"tests/functions/simple_test.tspp"
			],
			"problemMatcher": [
				"$gcc"
			]
		},
		{
			"label": "Build tspp",
			"type": "shell",
			"command": "./build.sh",
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}