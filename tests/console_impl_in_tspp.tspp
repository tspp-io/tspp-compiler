import sys;

class console {
  static function log(msg: string) {
    // Simple implementation using system_call to echo message to stdout.
    // Note: system_call uses shell; for portability, a more direct syscall can
    // be added to runtime (like tspp_sys_write_stdout) if desired.
    let c = "echo ";
    sys.system_call(c + msg);
  }
}

function main() {
  console.log(sys.get_platform());
}
