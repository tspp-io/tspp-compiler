// Build-only: ensure the parser treats <...> as type arguments only when followed by (, ., or [

function id<T>(x: T): T { return x; }

class C {
  function method(x: int): int { return x; }
}

function main(): int {
  let a: int = 0;
  // generic function call
  a = id<int>(1);
  // chained call with generics on method
  let obj: C;
  obj.method<int>(a);
  return 0;
}
