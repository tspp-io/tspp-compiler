import sys;

function main() {
  // File I/O: write then read
  sys.write_file("temp_test_io.txt", "hello-tspp /n writing file", 0);
  let content = sys.read_file("temp_test_io.txt");
  console.log(content);

  // CWD
  console.log(sys.get_cwd());

  // Environment variable (may be empty)
  console.log(sys.get_env("PATH"));
}
